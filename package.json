{
  "name": "nft-wallet",
  "version": "1.2.0",
  "description": "WholeLot NFT and Wallet Libraries, Connect multiple Wallets with React Component",
  "scripts": {
    "copy": "cp src/*.css lib/cjs && cp src/*.d.ts lib/cjs && cp src/*.css lib/esm &&  cp src/*.d.ts lib/esm",
    "build": "npm run clean && npm run  build:esm && npm run build:cjs && npm run copy",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "clean": "rimraf lib"
  },
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "/lib",
    "README.md"
  ],
  "keywords": [
    "NFT",
    "Wallet",
    "Ethereum",
    "Bit Coin",
    "Bit",
    "Coin"
  ],
  "author": {
    "name": "Sireesh Pangaluri",
    "email": "pangaluri2010@gmail.com",
    "url": "https://github.com/ineffablep"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ineffablep/nft-wallet.git"
  },
  "bugs": {
    "url": "https://github.com/ineffable/nft-wallet/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/coinbase": "^2.0.6",
    "@types/qrcode.react": "^1.0.1",
    "@types/react-router-dom": "^5.3.0",
    "@types/uuid": "^8.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@web3-react/authereum-connector": "^6.2.0",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/lattice-connector": "^6.1.9",
    "@web3-react/ledger-connector": "^6.1.9",
    "@web3-react/network-connector": "^6.1.9",
    "@web3-react/trezor-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.0",
    "@web3-react/walletlink-connector": "^6.2.2",
    "bitski": "^0.11.0-beta.4",
    "caver-js": "^1.6.3",
    "coinbase": "^2.0.8",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "uuid": "^8.3.2",
    "web3": "^1.4.0",
    "web3-core": "^1.4.0"
  },
  "dependencies": {
    "@portis/web3": "^4.0.5",
    "@wholelot/util": "^1.0.1",
    "react-router-dom": "^5.3.0"
  }
}
